<!-- Delete the Vehicle Details Of Transaction Family "Government" page -->

<center>
  <h3> Delete Vehicle Details </h3>
  <img src="/images/icon.png" height="75px">
  <div class="row">
    <div class="col s6 offset-s3"
      style="background-color: rgb(240, 232, 232); opacity: 1.0; height: 300px; padding: 20px 20px; border: 1px solid; overflow: scroll; overflow-x: hidden; overflow-y: scroll;">
      <form autocomplete="off">
        <div class="formtext">
          <br>
          <label style="font-size:16px; float:left; color:black;">Enter Private Key</label>
          <input type="text" id="private9" required>
          <label style="font-size:16px; float:left; color:black;">Enter Vin Number</label>
          <input type="text" id="vin9" required>
        </div>
        <div>
          <br />
          <button type="submit" onclick="deleteData1(event)" style="background-color: #0066ff"
            class="waves-effect waves-light btn-small">Delete</button>
          <br>
          <button type="reset" style="float: right; background-color: #ff0000"
            class="waves-effect waves-light btn-small">Clear</button>
        </div>
      </form>
    </div>
  </div>
</center>